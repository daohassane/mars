{
    "name": "mars/mars",
    "description": "Mars Framework",
    "keywords": ["framework", "mars", "mars framework", "micro framework"],
    "type": "project",
    "license": "MIT",
    "require": {
        "php": ">=7.0.0",
        "mars/framework": "dev-master"
    },
    "require-dev": {
        "phpunit/phpunit": "~6.0"
    },
    "autoload": {
        "psr-4": {
            "App\\": "app"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "config": {
        "preferred-install": "dist",
        "sort-packages": true,
        "optimize-autoloader": true
    }
}
